  "homepage": "https://github.com/Arylo/hexo-reslink#readme",
  "devDependencies": {
    "@types/faker": "^4.1.2",
    "@types/mocha": "^2.2.45",
    "@types/should": "^11.2.0",
    "faker": "^4.1.0",
    "mocha": "^4.0.1",
    "nyc": "^11.4.1",
    "should": "^12.0.0",
    "source-map-support": "^0.5.0",
